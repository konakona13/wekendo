<?xml version="1.0" encoding="UTF-8"?>
<beans
	xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation=
			"http://www.springframework.org/schema/beans
			http://www.springframework.org/schema/beans/spring-beans.xsd
			"	
>

<!-- Controller -->
<bean class = "Controller.YYYController.GoodsRegController" />
<bean class = "Controller.YYYController.DominoController" />
<bean class = "Controller.YYYController.GoodsListController" />
<bean class = "Controller.YYYController.ConfirmController" />
<bean class = "Controller.YYYController.ReviewController" />


<!-- Command -->
<bean class = "Command.YYYCommand.PlaceRegCommand" />
<bean class = "Command.YYYCommand.AuthInfoCommand" />
<bean class = "Command.YYYCommand.MapThemeCommand" />

<!-- DTO -->
<bean id = "mapLarge"  class="Model.DTO.YYYDTO.MapLarge" />
<bean id = "mapMedium"  class="Model.DTO.YYYDTO.MapMedium" />
<bean id = "mapSmall"  class="Model.DTO.YYYDTO.MapSmall" />
<bean id = "themeLarge"  class="Model.DTO.YYYDTO.ThemeLarge" />
<bean id = "themeMedium"  class="Model.DTO.YYYDTO.ThemeMedium" />
<bean id = "themeSmall"  class="Model.DTO.YYYDTO.ThemeSmall" />

<bean id = "goods"  class="Model.DTO.YYYDTO.Goods" />
<bean id = "goodsImg"  class="Model.DTO.YYYDTO.GoodsImg" /> 
<bean id = "company"  class="Model.DTO.YYYDTO.Company" />
<bean id = "goodsList"  class="Model.DTO.YYYDTO.GoodsList" />

</beans>